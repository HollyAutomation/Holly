{
  "name": "holly-ui",
  "version": "0.0.1",
  "scripts": {
    "ci": "run-s prettier:check ci:test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:watch": "run-s start",
    "test": "react-scripts test",
    "ci:test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "prettier:check": "prettier --ignore-path ../../.prettierignore --check **/*.{md,js,ts,tsx,json,yml}",
    "prettier:write": "prettier --ignore-path ../../.prettierignore --write **/*.{md,js,ts,tsx,json,yml}"
  },
  "files": [
    "build/**",
    "index.js"
  ],
  "main": "index.js",
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "8.1.0",
    "@types/jest": "25.1.2",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/reactstrap": "8.4.1",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "bootstrap": "4.4.1",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.3.1",
    "reactstrap": "8.4.1",
    "rimraf": "3.0.2",
    "typescript": "3.7.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "text-summary",
      "json"
    ]
  }
}
