{
  "name": "holly-ui",
  "version": "0.0.1",
  "scripts": {
    "ci": "run-s prettier:check ci:test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "ci:test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "prettier:check": "prettier --ignore-path ../../.prettierignore --check **/*.{md,js,json,yml}",
    "prettier:write": "prettier --ignore-path ../../.prettierignore --write **/*.{md,js,json,yml}"
  },
  "devDependencies": {
    "@types/jest": "24.0.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/reactstrap": "8.4.1",
    "bootstrap": "4.4.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.3.0",
    "reactstrap": "8.4.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "text-summary",
      "json"
    ]
  }
}
