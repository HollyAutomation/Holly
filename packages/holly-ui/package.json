{
  "name": "holly-ui",
  "version": "0.0.1",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-app-rewired build",
    "build:watch": "run-s start",
    "eject": "react-scripts eject",
    "lint": "run-s prettier:check",
    "prettier:check": "prettier --ignore-path ../../.prettierignore --check **/*.{md,js,ts,tsx,json,yml}",
    "prettier:write": "prettier --ignore-path ../../.prettierignore --write **/*.{md,js,ts,tsx,json,yml}"
  },
  "files": [
    "build/**",
    "index.js"
  ],
  "main": "index.js",
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.2",
    "@testing-library/user-event": "10.0.0",
    "@types/jest": "26.0.16",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/reactstrap": "8.4.2",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "bootstrap": "4.4.1",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "holly-shared": "0.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react": "16.13.1",
    "react-app-rewired": "2.1.5",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.1",
    "reactstrap": "8.4.1",
    "rimraf": "3.0.2",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "text-summary",
      "json"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.2.5"
  }
}
