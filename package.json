{
  "name": "holly",
  "version": "0.0.1",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "run-s test:passes test:failures test:coverage test:codecov",
    "test:passes": "nyc --silent bin/holly -c integration/passes/.hollyrc",
    "test:failures": "nyc --silent --no-clean bin/holly -c integration/fails/.hollyrc",
    "test:coverage": "nyc report --reporter=lcov --reporter=text --reporter=html",
    "test:codecov": "codecov",
    "lint": "eslint . --ext=ts,js",
    "build": "tsc -p lib",
    "build:watch": "tsc --watch -p lib"
  },
  "author": "Luke Page",
  "license": "ISC",
  "bin": {
    "holly": "./bin/holly"
  },
  "nyc": {
    "include": [
      "build",
      "lib"
    ]
  },
  "dependencies": {
    "@babel/traverse": "7.8.4",
    "debug": "4.1.1",
    "expect": "25.1.0",
    "glob": "7.1.6",
    "jest-snapshot": "25.1.0",
    "mocha": "7.0.1",
    "playwright": "0.9.24",
    "stack-utils": "2.0.1",
    "yargs": "15.1.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "7.0.8",
    "@types/debug": "4.1.5",
    "@types/glob": "7.1.1",
    "@types/koa": "2.11.0",
    "@types/mocha": "7.0.1",
    "@types/node": "13.5.3",
    "@types/prettier": "1.19.0",
    "@types/yargs": "15.0.2",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "eslint": "6.8.0",
    "koa": "2.11.0",
    "npm-run-all": "^4.1.5",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
